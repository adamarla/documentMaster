<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions targetNamespace="http://gutenberg/blocs" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://gutenberg/blocs" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
  <wsdl:types>
    <xsd:schema elementFormDefault="qualified" targetNamespace="http://gutenberg/blocs">			
			<xsd:complexType name="entryType">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:string"/>
					<xsd:element maxOccurs="1" minOccurs="0" name="name" type="xsd:string">
                    </xsd:element>
					<xsd:element maxOccurs="1" minOccurs="0" name="value" type="xsd:string">
                    </xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="quizType">
				<xsd:sequence>
					<xsd:element name="quiz" type="tns:entryType"/>
					<xsd:element name="teacher" type="tns:entryType"/>
					<xsd:element name="school" type="tns:entryType"/>
					<xsd:element maxOccurs="unbounded" minOccurs="1" name="page" type="tns:pageType">					
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="manifestType">
				<xsd:sequence>
					<xsd:element name="root" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="image" type="tns:entryType">
					</xsd:element>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="document" type="tns:entryType">
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="pageType">
				<xsd:sequence>
					<xsd:element name="number" type="xsd:int"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="question" type="tns:entryType">
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="responseType">
				<xsd:choice>
					<xsd:element name="manifest" type="tns:manifestType">
					</xsd:element>
					<xsd:element name="error" type="xsd:string"/>
				</xsd:choice>
			</xsd:complexType>

			<xsd:complexType name="assignmentType">
				<xsd:sequence>
					<xsd:element name="quiz" type="tns:entryType"/>
					<xsd:element name="instance" type="tns:entryType"/>
					<xsd:element maxOccurs="unbounded" minOccurs="1" name="students" type="tns:entryType">
					</xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="annotationType">
				<xsd:sequence>
					<xsd:element name="scanId" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="coordinates" type="tns:pointType"/>
				</xsd:sequence>
			</xsd:complexType>
						
            <xsd:complexType name="rotateScanRequestType">
                <xsd:sequence>
                    <xsd:element name="scanId" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
                        
			<xsd:complexType name="pointType">
				<xsd:sequence>
					<xsd:element name="x" type="xsd:int"/>
					<xsd:element name="y" type="xsd:int"/>
					<xsd:element minOccurs="0" name="code" type="xsd:int"/>
					<xsd:element minOccurs="0" name="text" type="xsd:string"/>
				</xsd:sequence>			
			</xsd:complexType>
			
			<xsd:complexType name="questionTagsType">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" name="marks" type="xsd:int"/>
					<xsd:element maxOccurs="unbounded" name="length" type="tns:lengthType"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="breaks" type="xsd:int"/>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:simpleType name="lengthType">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="fullpage"/>
					<xsd:enumeration value="halfpage"/>
					<xsd:enumeration value="fewlines"/>
					<xsd:enumeration value="mcq"/>
				</xsd:restriction>
			</xsd:simpleType>
						
			<xsd:complexType name="teacherType">
			  <xsd:sequence>
			    <xsd:element name="teacher" type="tns:entryType"/>
			    <xsd:element name="school" type="tns:entryType"/>
			  </xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="studentGroupType">
			  <xsd:sequence>
			    <xsd:element name="school" type="tns:entryType"/>
			    <xsd:element name="group" type="tns:entryType"/>
			    <xsd:element maxOccurs="unbounded" minOccurs="0" name="members" type="tns:entryType"/>
			  </xsd:sequence>
			</xsd:complexType>
						
			<xsd:element name="buildQuiz" type="tns:quizType">
			</xsd:element>
			<xsd:element name="buildQuizResponse" type="tns:responseType">
			</xsd:element>
			
			<xsd:element name="assignQuiz" type="tns:assignmentType"/>

			<xsd:element name="assignQuizResponse" type="tns:responseType">
			</xsd:element>

			<xsd:element name="receiveScans" type="xsd:string"/>

			<xsd:element name="receiveScansResponse" type="tns:responseType">
			</xsd:element>

			<xsd:element name="createQuestion" type="xsd:string"/>

			<xsd:element name="createQuestionResponse" type="tns:responseType">
			</xsd:element>

			<xsd:element name="annotateScan" type="tns:annotationType"/>

			<xsd:element name="annotateScanResponse" type="tns:responseType">
			</xsd:element>

            <xsd:element name="rotateScan" type="tns:rotateScanRequestType"/>

            <xsd:element name="rotateScanResponse" type="tns:responseType">
            </xsd:element>
            
			<xsd:element name="tagQuestion" type="tns:questionTagsType"/>

			<xsd:element name="tagQuestionResponse" type="tns:responseType">
			</xsd:element>
						
			<xsd:element name="generateSuggestionForm" type="tns:teacherType"/>

			<xsd:element name="generateSuggestionFormResponse" type="tns:responseType">
			</xsd:element>
			
			<xsd:element name="generateStudentRoster" type="tns:studentGroupType"/>

			<xsd:element name="generateStudentRosterResponse" type="tns:responseType">
			</xsd:element>
			
            <xsd:element name="generateQuizReport" type="tns:studentGroupType"/>

            <xsd:element name="generateQuizReportResponse" type="tns:responseType">
            </xsd:element>
            
		</xsd:schema>
  </wsdl:types>
